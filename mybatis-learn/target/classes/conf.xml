				<property name="url" value="${url}" />
				<property name="username" value="ics_cbs_calengine" />
				<property name="password" value="ics_cbs_calengine" />
			</dataSource>
		</environment>
	</environments>
	<mappers>
		<mapper resource="mapper.xml"></mapper>
	</mappers>
